{
  "name": "nature-remo",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "dev": "nodemon src/app.ts --watch ./src",
    "start": "node dist/app.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": [
      "ts"
    ],
    "ignore": [
      "*.test.ts"
    ],
    "delay": "1",
    "execMap": {
      "ts": "ts-node"
    }
  }
}
